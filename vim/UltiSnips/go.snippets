snippet ttest "Table-based test"
func Test${1:Class}${2:Func}(t *testing.T) {
	tests := []struct{
		$3
	}{
		$5
	}
	for i, tt := range tests {
		$4
		if got, want := got, tt.want; !reflect.DeepEqual(got, want) {
			t.Errorf("%d $1.$2()\ngot  %#v\nwant %#v", i, got, want)
		}
	}
}
endsnippet

snippet ttspew "Table-based test with spew output"
func Test${1:Class}${2:Func}(t *testing.T) {
	tests := []struct{
		$3
	}{
		$5
	}
	for i, tt := range tests {
		$4
		if got, want := got, tt.want; !reflect.DeepEqual(got, want) {
			t.Errorf("%d $1.$2()\ngot\n%swant\n%s", i, spew.Sdump(got), spew.Sdump(want))
		}
	}
}
endsnippet

